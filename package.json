{
  "name": "react-mobx-ts",
  "version": "1.0.3",
  "description": "react16+react-router4+mobx",
  "pre-commit": [
    "lint"
  ],
  "scripts": {
    "start": "better-npm-run start",
    "clean": "rimraf dist",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:qa": "better-npm-run deploy:qa",
    "start:dist": "better-npm-run start:dist",
    "prod": "better-npm-run prod",
    "dev": "better-npm-run dev",
    "qa": "better-npm-run qa",
    "publish:setup": "./node_modules/.bin/pm2 deploy ecosystem.config.js development setup",
    "publish": "./node_modules/.bin/pm2 deploy ecosystem.config.js development",
    "test": "cross-env NODE_ENV=development karma start build/karma.conf.js",
    "test:watch": "npm run test --watch",
    "lint:fix": "npm run lint -- --fix",
    "lint": "./node_modules/.bin/tslint -c tslint.json -p tsconfig.json --fix --format stylish"
  },
  "betterScripts": {
    "start": {
      "command": "ts-node build/index.ts",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start:dist": {
      "command": "ts-node build/index.ts",
      "env": {
        "NODE_ENV": "production",
        "PORT": "8012",
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run clean && ts-node build/compile.ts",
      "env": {
        "NODE_ENV": "development",
        "PORT": "8010",
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "npm run deploy:dev && npm run start:dist",
      "env": {
        "NODE_ENV": "development",
        "PORT": "8010",
        "DEBUG": "app:*"
      }
    },
    "deploy:qa": {
      "command": "npm run clean && ts-node build/compile.ts",
      "env": {
        "NODE_ENV": "qa",
        "PORT": "8011",
        "DEBUG": "app:*"
      }
    },
    "qa": {
      "command": "npm run deploy:qa && npm run start:dist",
      "env": {
        "NODE_ENV": "qa",
        "PORT": "8011",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && ts-node build/compile.ts",
      "env": {
        "NODE_ENV": "production",
        "PORT": "8012",
        "DEBUG": "app:*"
      }
    },
    "prod": {
      "command": "npm run deploy && npm run start:dist",
      "env": {
        "NODE_ENV": "production",
        "PORT": "8012",
        "DEBUG": "app:*"
      }
    }
  },
  "author": "shienzone",
  "license": "MIT",
  "dependencies": {
    "antd": "^3.1.0",
    "axios": "^0.17.1",
    "core-js": "^2.5.3",
    "debug": "^3.1.0",
    "echarts": "^3.8.5",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "mobx-react-router": "^4.0.1",
    "moment": "^2.20.1",
    "ramda": "^0.25.0",
    "normalize.css": "^7.0.0",
    "react": "^16.2.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-immutable-render-mixin": "^0.9.7",
    "react-loadable": "^5.3.1",
    "react-router-dom": "^4.2.2",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/autoprefixer": "^6.7.3",
    "@types/express": "^4.0.39",
    "@types/history": "^4.6.2",
    "@types/lodash": "^4.14.91",
    "@types/node": "^8.5.5",
    "@types/ramda": "^0.25.13",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-copy-to-clipboard": "^4.2.3",
    "@types/webpack": "^3.8.2",
    "@types/webpack-env": "^1.13.3",
    "autoprefixer": "^7.2.4",
    "awesome-typescript-loader": "^3.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "compression": "^1.7.1",
